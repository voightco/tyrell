    // Code spell checker
    "streetsidesoftware.code-spell-checker",

    // Shows image preview in the gutter and on hover
    "kisstkondoros.vscode-gutter-preview",

    // Markdown Word Count
    "ms-vscode.wordcount"
    
    // Insert date strings
    "jsynowiec.vscode-insert-date-string"

