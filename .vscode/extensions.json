{
  // These are all the necesseray extensions for this workspace

  "recommendations": [
    // Foam's own extension
    "foam.foam-vscode",

    // GitLens for seeing version history inline
    "eamodio.gitlens",

    // Tons of markdown goodies (lists, tables of content, so much more)
    "yzhang.markdown-all-in-one",

    // [[wiki-links]], backlinking etc
    "kortina.vscode-markdown-notes",

    // Adds `show graph` command that displays graph of linked notes
    "tchayen.markdown-links",

    // Runs commands when VSCode starts
    "gabrielgrinberg.auto-run-command",

    // Draw.io integration
    "hediet.vscode-drawio",
    
    // Expands regular links into Markdown Links
    "skn0tt.markdown-link-expander"

    // See http://go.microsoft.com/fwlink/?LinkId=827846
    // for the documentation about the extensions.json format
  ]
}
